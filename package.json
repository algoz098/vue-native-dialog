{
  "name": "vue-native-dialog",
  "private": false,
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-native-dialog.umd.js",
  "module": "./dist/vue-native-dialog.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-native-dialog.es.js",
      "require": "./dist/vue-native-dialog.umd.js"
    }
  },
  "scripts": {
    "docs:dev": "vitepress dev docs-src",
    "docs:build": "vitepress build docs-src && cp -Rf docs-src/.vitepress/dist/* docs/",
    "docs:serve": "vitepress serve docs",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore . --config .eslintrc.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.0",
    "vitepress": "^0.22.3",
    "vue-tsc": "^0.29.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run docs:build"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix"
  }
}
